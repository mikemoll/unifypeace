#beta-container.fixed-on-top
  #map style="z-index:-99"

div#content-for-map data-contents="#{@markers}"
div#location-for-map data-contents="#{[@location["latitude"], @location["longitude"]]}"

= link_to image_tag("markers/medi-icon.png"), "#", class: "subcategory", data: {filter: "meditation"}
