- content_for :title do
  = "Unify"
- content_for :javascript do
  = javascript_include_tag :maps

#beta-container.fixed-on-top
  #map

div#content-for-map data-contents="#{@markers}"
div#location-for-map data-contents="#{[@location["latitude"], @location["longitude"]]}"

= link_to image_tag("markers/medi-icon.png"), "#", class: "subcategory", data: {filter: "meditation"}
= link_to image_tag("markers/musi-icon.png"), "#", class: "subcategory", data: {filter: "music"}
= link_to image_tag("markers/march-icon.png"), "#", class: "subcategory", data: {filter: "march"}
= link_to image_tag("markers/multi-icon.png"), "#", class: "subcategory", data: {filter: "multi"}